<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uap.TransaksiController">
   <children>
      <Label layoutX="537.0" layoutY="364.0" text="Transaksi" />
      <TableView fx:id="tableView" layoutX="9.0" layoutY="9.0" prefHeight="165.0" prefWidth="581.0">
        <columns>
          <TableColumn fx:id="colId" prefWidth="124.0" text="ID" />
          <TableColumn fx:id="colJumlah" prefWidth="71.0" text="Jumlah" />
            <TableColumn fx:id="colStok" prefWidth="86.0" text="Stok" />
            <TableColumn fx:id="colProduk" prefWidth="19.0" text="Produk" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <TextField fx:id="id" layoutX="209.0" layoutY="179.0" />
      <TextField fx:id="jumlah" layoutX="208.0" layoutY="213.0" />
      <TextField fx:id="stok" layoutX="209.0" layoutY="245.0" />
      <Label layoutX="43.0" layoutY="183.0" text="ID Transaksi" />
      <Label layoutX="56.0" layoutY="217.0" text="Jumlah" />
      <Label layoutX="63.0" layoutY="249.0" text="Stok" />
      <TextField fx:id="produk" layoutX="208.0" layoutY="278.0" />
      <Label layoutX="39.0" layoutY="282.0" text="Nama Produk" />
      <Button fx:id="btnBack" layoutX="10.0" layoutY="360.0" mnemonicParsing="false" onAction="#back" text="Kembali" />
      <Button fx:id="btnTambah" layoutX="297.0" layoutY="323.0" mnemonicParsing="false" onAction="#tambahData" text="Tambah" />
      <Button fx:id="btnHapus" layoutX="107.0" layoutY="360.0" mnemonicParsing="false" onAction="#openHapus" text="Hapus Formulir" />
   </children>
</AnchorPane>
